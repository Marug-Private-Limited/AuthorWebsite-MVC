        });

        $(document).on('click', '#viewBtn', function (ev) {
            var id = $(this).attr("data-id");
            var title = $(this).attr("data-title");
            var short_desc = $(this).attr("data-desc");
            var long_desc = $(this).attr("data-longdesc");
            var img_link = $(this).attr("data-img");
            var lik = $(this).attr("data-youtube");
            var cmt = $(this).attr("data-youtube");
            var vie = $(this).attr("data-youtube");
            var yt_link = $(this).attr("data-youtube");
            var cdate = $(this).attr("data-cdate");
            var udate = $(this).attr("data-udate");
           
            $('#titleSpan').html(title);
            /*document.getElementById("viewModalId").value = id;*/
            document.getElementById("viewModalTitle").value = title;
            document.getElementById("viewModalDesc").value = long_desc;
            document.getElementById("viewModalLongDesc").value = short_desc;
            document.getElementById("viewModalImg").value = img_link;
            document.getElementById("viewModalYoutube").value = yt_link;
            document.getElementById("viewModalCdate").value = cdate;
           
            $(document).on('click', '#insertData', function (ev) {
               var post_title = $('#viewModalTitle').val();
                var short_description = $('#short_description').val();
                var long_description = $('#viewModalLongDesc').val();
                var post_image = $('#viewModalImg').val();
                var post_youtube = $('#viewModalYoutube').val();
                var created_on = $('#viewModalCdate').val();

                var empObj = {
                    post_title: post_title,
                    long_description: long_description,
                    short_description: short_description,
                    post_image: post_image,
                    post_youtube: post_youtube,
                    created_on: created_on,
                };
                $.ajax({
                    
                    type: "POST",
                    url: "/Admin/Edit/" + id,
                    data: JSON.stringify(empObj),
                    dataType: 'json',
                    contentType: false,
                    processData: false,
                    success: function (response) {
                        alert(response.responseText);
                        location.reload();
                    },
                    error: function (error) {

                    }
                });
             });
        });
        $(document).on('click', '#delBtn', function (ev) {
            var id = $(this).attr("data-id");
            var title = $(this).attr("data-title");
            $('#deltitleSpan').html(title);
            $('#delSpan').html(title);
            var empObj = {
                id: id
            };
            $(document).on('click', '#delData', function (ev) {
              //  alert(id);
                $.ajax({
                    type: "POST",
                    url: "/Admin/Delete/" + id,
                  //  data: empObj,
                    dataType: 'json',
                    contentType: false,
